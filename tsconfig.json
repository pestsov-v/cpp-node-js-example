{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strict": true,
    "newLine": "lf",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": "./",
    "rootDir": "./src",
  },
  "include": ["./src/**/*",],
  "exclude": ["node_modules"],
}
